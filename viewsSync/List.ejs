<button id="add" data-event="click" data-action="addListItem" style="display: inline-block;">add</button>
<button id="del" data-event="click" data-action="delListItem" style="display: inline-block;">del</button>
<ul id="items">
    <% for( let index = 0; index < items.length; index++ ) { %>
        <%- include('/views/ListElement.ejs', { name: items[index] }) %>
    <% } %>
</ul>